<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>my.group</groupId>
  <artifactId>my-test</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name>My Odi Project</name>
  <properties>
    <sonar.jdbc.username>sonar</sonar.jdbc.username>
    <sonar.odi.jdbc.url>jdbc</sonar.odi.jdbc.url>
    <sonar.dynamicAnalysis>false</sonar.dynamicAnalysis>
    <sonar.odi.password.secured>password</sonar.odi.password.secured>
    <sonar.language>odi</sonar.language>
    <sonar.jdbc.password>sonar</sonar.jdbc.password>
    <sonar.jdbc.url>jdbc:mysql://localhost:3306/sonar</sonar.jdbc.url>
    <sonar.host.url>http://127.0.0.1:8080/sonar</sonar.host.url>
