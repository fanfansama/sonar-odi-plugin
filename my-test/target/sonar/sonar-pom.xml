    <scriptSourceDirectory>F:\JAVA\workspace-sonar\my-test\src\main\scripts</scriptSourceDirectory>
    <testSourceDirectory>F:\JAVA\workspace-sonar\my-test\src\test\java</testSourceDirectory>
    <outputDirectory>F:\JAVA\workspace-sonar\my-test\target\classes</outputDirectory>
    <testOutputDirectory>F:\JAVA\workspace-sonar\my-test\target\test-classes</testOutputDirectory>
    <resources>
